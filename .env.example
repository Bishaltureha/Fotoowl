# Base URL for the FotoOwl API
EXPO_PUBLIC_API_BASE_URL=https://openapi.fotoowl.ai/open/event/image-list


# Your FotoOwl API key (required)
EXPO_PUBLIC_API_KEY=your_api_key_here


# Event ID for fetching images (required)
EXPO_PUBLIC_EVENT_ID=your_event_id_here


# API Configuration
EXPO_PUBLIC_DEFAULT_PAGE_SIZE=40
EXPO_PUBLIC_DEFAULT_TIMEOUT=10000
EXPO_PUBLIC_MAX_RETRIES=3
EXPO_PUBLIC_RETRY_DELAY=1000


# App Configuration
EXPO_PUBLIC_APP_NAME=FotoOwl
EXPO_PUBLIC_APP_VERSION=1.0.0


# Development Configuration
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_ENABLE_LOGGING=false